<div class="container-fluid">
    <div class="row">
        {{#each categories}}
        <div class="col-12">
            <div class="card search-result">
                <div class="row">
                    <div class="col-12">
                        <a class="card-title" href="/category_details/{{this.CategoryID}}">{{this.Name}}</a>
                    </div>
                    <div class="col-12">
                        <span class="card-small-text">Created: {{this.CreatedDate}}</span>
                    </div>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>